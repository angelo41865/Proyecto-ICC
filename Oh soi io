elif N > 30:
    print("Usted tiene obesidad, su IMC es:", int(indiceMasaCorporal(peso, talla)))
    print("Una dieta ideal para usted seria:")
    for r in dieta_obesidad:
        print(" ")
        print(r)
    dia = int(input("Inserte dia de rutina:"))
    if dia >= 6:
        print("Inserte su dia de rutina")
        dia = int(input("Inserte dia de rutina:"))

    if dia==1:
        print("Debe realizar la siguiente rutina:")
        a = random.sample(torso, 2)
        b = random.sample(cardio, 3)
        c = random.sample(piernas, 3)
        d = random.sample(brazos, 3)
        for item in a:
            print(item)
        for cos in b:
            print(cos)
        for sin in c:
            print(sin)
            for tan in d:
                print(tan)
        print("Repetir 1 vez")
    if dia == 2:
        print("Debe realizar la siguiente rutina:")
